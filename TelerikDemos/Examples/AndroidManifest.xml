<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telerik.qsf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.telerik.qsf.common.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/telerik_logo"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.telerik.qsf.common.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/BrandedActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.telerik.qsf.common.FallbackExamples"
            android:label="@string/title_activity_default_examples" />
        <activity
            android:name="com.telerik.qsf.common.ExampleGroupActivity"
            android:label="" />
        <activity
            android:name="com.telerik.qsf.common.ExamplesActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label=""
            android:theme="@style/DarkTheme" />
        <activity
            android:name="com.telerik.qsf.common.ExampleInfoActivity"
            android:label="@string/title_activity_example_info"></activity>
    </application>

</manifest>
